/* Mensajes de combo y orden en el HUD */
.hud-combo-message {
  position: absolute;
  right: 15px;
  top: 50%;
  transform: translateY(-50%);
  animation: hudMessageSlide 0.5s ease-out;
}

.hud-order-message {
  position: absolute;
  left: 15px;
  top: 50%;
  transform: translateY(-50%);
  animation: hudMessageSlide 0.5s ease-out;
}

@keyframes hudMessageSlide {
  0% {
    opacity: 0;
    transform: translateY(-50%) scale(0.3) rotate(180deg);
  }
  50% {
    opacity: 0.8;
    transform: translateY(-50%) scale(1.3) rotate(10deg);
  }
  100% {
    opacity: 1;
    transform: translateY(-50%) scale(1) rotate(0deg);
  }
}

.hud-combo-content {
  background: linear-gradient(45deg, rgba(255, 215, 0, 0.95), rgba(255, 140, 0, 0.95), rgba(255, 69, 0, 0.9));
  border: 3px solid #ffd700;
  border-radius: 20px;
  padding: 12px 20px;
  text-align: center;
  box-shadow: 
    0 0 30px rgba(255, 215, 0, 0.8),
    0 0 60px rgba(255, 215, 0, 0.4),
    inset 0 0 20px rgba(255, 255, 255, 0.3);
  color: #000;
  font-family: "Press Start 2P", monospace;
  font-size: 12px;
  font-weight: normal;
  text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.7);
  white-space: nowrap;
  animation: hudComboGlow 0.6s ease-in-out infinite alternate;
  transform: rotate(-2deg);
  min-width: 120px;
}

.hud-order-content {
  background: linear-gradient(45deg, rgba(0, 255, 127, 0.95), rgba(0, 191, 255, 0.95), rgba(0, 150, 255, 0.9));
  border: 3px solid #00ff7f;
  border-radius: 20px;
  padding: 12px 20px;
  text-align: center;
  box-shadow: 
    0 0 30px rgba(0, 255, 127, 0.8),
    0 0 60px rgba(0, 255, 127, 0.4),
    inset 0 0 20px rgba(255, 255, 255, 0.3);
  color: #000;
  font-family: "Press Start 2P", monospace;
  font-size: 12px;
  font-weight: normal;
  text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.8);
  white-space: nowrap;
  animation: hudOrderGlow 0.6s ease-in-out infinite alternate;
  transform: rotate(2deg);
  min-width: 120px;
}

@keyframes hudComboGlow {
  0% {
    box-shadow: 
      0 0 30px rgba(255, 215, 0, 0.8),
      0 0 60px rgba(255, 215, 0, 0.4),
      inset 0 0 20px rgba(255, 255, 255, 0.3);
    border-color: #ffd700;
    transform: rotate(-2deg) scale(1) translateY(0);
  }
  50% {
    box-shadow: 
      0 0 50px rgba(255, 215, 0, 1),
      0 0 100px rgba(255, 215, 0, 0.6),
      inset 0 0 30px rgba(255, 255, 255, 0.5);
    border-color: #ffed4e;
    transform: rotate(2deg) scale(1.1) translateY(-2px);
  }
  100% {
    box-shadow: 
      0 0 40px rgba(255, 215, 0, 0.9),
      0 0 80px rgba(255, 215, 0, 0.5),
      inset 0 0 25px rgba(255, 255, 255, 0.4);
    border-color: #ffaa00;
    transform: rotate(-1deg) scale(1.05) translateY(-1px);
  }
}

@keyframes hudOrderGlow {
  0% {
    box-shadow: 
      0 0 30px rgba(0, 255, 127, 0.8),
      0 0 60px rgba(0, 255, 127, 0.4),
      inset 0 0 20px rgba(255, 255, 255, 0.3);
    border-color: #00ff7f;
    transform: rotate(2deg) scale(1) translateY(0);
  }
  50% {
    box-shadow: 
      0 0 50px rgba(0, 255, 127, 1),
      0 0 100px rgba(0, 255, 127, 0.6),
      inset 0 0 30px rgba(255, 255, 255, 0.5);
    border-color: #40ffbf;
    transform: rotate(-2deg) scale(1.1) translateY(-2px);
  }
  100% {
    box-shadow: 
      0 0 40px rgba(0, 255, 127, 0.9),
      0 0 80px rgba(0, 255, 127, 0.5),
      inset 0 0 25px rgba(255, 255, 255, 0.4);
    border-color: #00dd77;
    transform: rotate(1deg) scale(1.05) translateY(-1px);
  }
}

