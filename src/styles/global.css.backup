/* Lives display con naves en miniatura */
.lives-display {
  background: rgba(0, 0, 0, 0.7);
  border: 2px solid #00ffff;
  padding: 15px 20px;
  border-radius: 15px;
  text-align: center;
  box-shadow: 
    0 0 15px rgba(0, 255, 255, 0.3),
    inset 0 0 10px rgba(0, 255, 255, 0.1);
}

.lives-display .instrument-label {
  font-size: 10px;
  color: #ffaa00;
  text-shadow: 0 0 5px #ffaa00;
