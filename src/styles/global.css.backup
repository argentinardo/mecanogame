/* ===== RESPONSIVE ADJUSTMENTS ===== */

@media (max-width: 1200px) {
  .integrated-control-panel {
    gap: 15px;
  }
  
  .control-section {
    max-width: 240px;
    min-width: 220px;
  }
  
  .control-section .hand-map {
    transform: scale(0.75);
  }
}

@media (max-width: 768px) {
  .integrated-control-panel {
    flex-direction: column;
    align-items: center;
    gap: 10px;
  }
  
  .control-section {
    max-width: 300px;
  }
  
  .control-section .hand-map {
    transform: scale(0.6);
  }
  
  .floating-combo-message,
  .floating-order-message {
    position: relative;
    top: auto;
    left: auto;
    right: auto;
    transform: none;
    margin: 10px;
  }
}

/* ===== EFECTOS DE EXPLOSIÓN NEON ESPECTACULARES ===== */

/* Ondas de choque neon */
.neon-shock-wave {
  pointer-events: none;
  filter: blur(1px);
  animation: neonShockPulse 0.1s ease-out infinite alternate;
}

@keyframes neonShockPulse {
  0% {
    filter: blur(1px) brightness(1);
  }
  100% {
    filter: blur(2px) brightness(1.3);
  }
}

/* Chispas neon principales */
.neon-spark {
  pointer-events: none;
  animation: neonSparkFlicker 0.1s ease-in-out infinite alternate;
}

@keyframes neonSparkFlicker {
  0% {
    filter: brightness(1) saturate(1);
  }
  100% {
    filter: brightness(1.4) saturate(1.5);
  }
}

/* Partículas brillantes con estela */
.glow-particle {
  pointer-events: none;
  filter: blur(0.5px);
  animation: glowParticleShimmer 0.2s ease-in-out infinite alternate;
}

@keyframes glowParticleShimmer {
  0% {
    filter: blur(0.5px) brightness(1.2);
  }
  100% {
    filter: blur(1px) brightness(1.8);
  }
}

/* Arcos eléctricos */
.electric-arc {
  pointer-events: none;
  filter: blur(0.8px);
  animation: electricFlicker 0.05s ease-in-out infinite;
}

@keyframes electricFlicker {
  0% {
    filter: blur(0.8px) brightness(1);
    transform: rotate(var(--arc-angle, 0deg)) scaleX(1);
  }
  20% {
    filter: blur(1.2px) brightness(1.5);
    transform: rotate(var(--arc-angle, 0deg)) scaleX(1.05);
  }
  40% {
    filter: blur(0.5px) brightness(2);
    transform: rotate(var(--arc-angle, 0deg)) scaleX(0.95);
  }
  60% {
    filter: blur(1px) brightness(1.2);
    transform: rotate(var(--arc-angle, 0deg)) scaleX(1.02);
  }
  80% {
    filter: blur(0.7px) brightness(1.8);
    transform: rotate(var(--arc-angle, 0deg)) scaleX(0.98);
  }
  100% {
    filter: blur(0.8px) brightness(1);
    transform: rotate(var(--arc-angle, 0deg)) scaleX(1);
  }
}

/* Anillos expansivos neon */
.neon-ring {
  pointer-events: none;
  filter: blur(1px);
  animation: neonRingPulse 0.3s ease-out infinite alternate;
}

@keyframes neonRingPulse {
  0% {
    filter: blur(1px) brightness(1);
  }
  50% {
    filter: blur(1.5px) brightness(1.5);
  }
  100% {
    filter: blur(2px) brightness(1.2);
  }
}

/* Ondas de choque de meteoritos */
.meteorite-shock-wave {
  pointer-events: none;
  filter: blur(2px);
  animation: meteoriteShockFlash 0.1s ease-out infinite alternate;
}

@keyframes meteoriteShockFlash {
  0% {
    filter: blur(2px) brightness(1) hue-rotate(0deg);
  }
  100% {
    filter: blur(3px) brightness(1.6) hue-rotate(20deg);
  }
}

/* Partículas de meteorito neon */
.meteorite-neon-particle {
  pointer-events: none;
  filter: blur(1px);
  animation: meteoriteNeonGlow 0.15s ease-in-out infinite alternate;
}

@keyframes meteoriteNeonGlow {
  0% {
    filter: blur(1px) brightness(1.2) contrast(1);
  }
  100% {
    filter: blur(1.5px) brightness(1.8) contrast(1.3);
  }
}

/* Efecto adicional para todas las partículas de explosión */
.neon-spark,
.glow-particle,
.electric-arc,
.neon-ring,
.neon-shock-wave,
.meteorite-shock-wave,
.meteorite-neon-particle {
  will-change: transform, opacity, filter;
}

/* Efectos especiales para múltiples explosiones simultáneas */
@keyframes explosionScreenFlash {
  0% {
    background: rgba(255, 255, 255, 0);
  }
  10% {
    background: rgba(255, 255, 255, 0.1);
  }
  20% {
    background: rgba(0, 255, 255, 0.05);
  }
  100% {
    background: rgba(255, 255, 255, 0);
  }
}

/* Partículas con mejor rendimiento usando CSS transforms */
.particle {
  pointer-events: none;
  will-change: transform, opacity;
  transform-origin: center;
}

/* Mejoras para partículas existentes */
.particle,
.meteorite-particle {
  animation: particleGlow 0.5s ease-out infinite alternate;
}

@keyframes particleGlow {
  0% {
    filter: brightness(1) blur(1px);
  }
  100% {
    filter: brightness(1.5) blur(2px);
  }
}

/* Cartel grande de sector */
.sector-info-large {
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    z-index: 1000; /* Aumentar el z-index para asegurar que esté por encima de todo */
    width: 80%;
    max-width: 800px;
    animation: sectorInfoAppear 0.5s ease-out;
}

.sector-panel-large {
    background: linear-gradient(145deg, rgba(0, 30, 60, 0.95), rgba(0, 20, 40, 0.98));
    border: 5px solid #00ffff;
    border-radius: 30px;
    padding: 40px;
    text-align: center;
    box-shadow: 
        0 0 50px rgba(0, 255, 255, 0.7),
        inset 0 0 30px rgba(0, 255, 255, 0.2);
    backdrop-filter: blur(10px);
    animation: sectorLargeGlow 3s ease-in-out infinite alternate;
}

.sector-label-large {
    font-size: 16px;
    color: #ffaa00;
    text-shadow: 0 0 10px #ffaa00;
    margin-bottom: 20px;
    font-family: "Press Start 2P", monospace;
    letter-spacing: 3px;
}

.sector-name-large {
    font-size: 36px;
    margin-bottom: 20px;
    color: #ffaa00;
    text-shadow: 0 0 25px #ffaa00;
    font-family: "Press Start 2P", monospace;
    font-weight: normal;
}

.sector-description-large {
    font-size: 18px;
    color: #00ffff;
    opacity: 0.9;
    font-family: "Press Start 2P", monospace;
    font-weight: normal;
    line-height: 1.5;
}

/* Agregar instrucción para cerrar */
.sector-panel-large::after {
    content: "Presiona cualquier tecla para continuar";
    display: block;
    font-size: 14px;
    color: #00ffff;
    margin-top: 30px;
    opacity: 0.8;
    font-family: "Press Start 2P", monospace;
    animation: blinkText 1.5s infinite;
}

@keyframes blinkText {
    0%, 100% { opacity: 0.8; }
    50% { opacity: 0.4; }
}

@keyframes sectorInfoAppear {
    0% {
        opacity: 0;
        transform: translate(-50%, -50%) scale(0.8);
    }
    100% {
        opacity: 1;
        transform: translate(-50%, -50%) scale(1);
    }
}

@keyframes sectorLargeGlow {
    0% {
        box-shadow: 0 0 50px rgba(0, 255, 255, 0.7), inset 0 0 30px rgba(0, 255, 255, 0.2);
        border-color: #00ffff;
    }
    100% {
        box-shadow: 0 0 80px rgba(0, 255, 255, 0.9), inset 0 0 50px rgba(0, 255, 255, 0.3);
        border-color: #66ffff;
    }
}

/* Estilos para iconos Memory de Pictogrammers */
.pixel-icon {
  filter: drop-shadow(0 0 2px currentColor);
  transition: all 0.3s ease;
  color: currentColor;
}

.btn-icon {
  margin-right: 8px;
  filter: drop-shadow(0 0 3px currentColor);
  transition: all 0.3s ease;
  color: currentColor;
}

.btn-icon:hover {
  filter: drop-shadow(0 0 5px currentColor) brightness(1.2);
  transform: scale(1.1);
}

/* Mejorar el renderizado de iconos Memory */
svg {
  image-rendering: pixelated;
  image-rendering: -moz-crisp-edges;
  image-rendering: crisp-edges;
}

/* Estilos específicos para iconos Memory */
.memory-icon {
  filter: drop-shadow(0 0 2px currentColor);
  transition: all 0.3s ease;
  color: currentColor;
}

.memory-icon:hover {
  filter: drop-shadow(0 0 4px currentColor) brightness(1.1);
  transform: scale(1.05);
}

/* Asegurar que los iconos Memory mantengan el color del tema */
.sound-toggle svg,
.start-btn svg,
.continue-btn svg {
  color: inherit;
  fill: currentColor;
}

/* Estilos de hover para el contenedor de instrucciones clickeable */
.instructions {
  transition: all 0.3s ease;
  cursor: pointer;
}

.instructions:hover {
  transform: translate(-50%, -50%) scale(1.02);
  box-shadow: 0 0 40px rgba(0, 255, 255, 0.6);
  border-color: #66ffff;
}

/* Estilos actualizados para el cannon centrado como en Asteroids */
.cannon {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 80px;
  height: 100px;
  z-index: 5;
  transition: transform 0.05s ease-out;
  animation: shipFloating 3s ease-in-out infinite;
}

/* Corrección del cannon-barrel para alineación correcta */
.cannon-barrel {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  width: 8px;
  height: 28px;
  background: linear-gradient(to top, #444, #777);
  border: 2px solid #1900ff;
  box-shadow: 0 0 15px rgba(0, 255, 255, 0.5);
  animation: cannonGlow 2s ease-in-out infinite alternate;
  z-index: 5;
  transform-origin: center bottom;
}

@keyframes shipFloating {
  0% {
    transform: translate(-50%, -50%) translate(0px, 0px);
  }
  25% {
    transform: translate(-50%, -50%) translate(8px, -8px);
  }
  50% {
    transform: translate(-50%, -50%) translate(0px, -12px);
  }
  75% {
    transform: translate(-50%, -50%) translate(-8px, -8px);
  }
  100% {
    transform: translate(-50%, -50%) translate(0px, 0px);
  }
}
