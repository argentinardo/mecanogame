}

.status-light.ready {
  color: #00ff00;
  text-shadow: 0 0 10px #00ff00;
}

.status-light.penalty {
  color: #ff0066;
  text-shadow: 0 0 10px #ff0066;
  animation: statusAlert 0.5s infinite alternate;
}

@keyframes statusAlert {
  0% { opacity: 0.7; }
  100% { opacity: 1; }
}

/* Ajustar el área de juego en móviles para el teclado */
@media (max-width: 768px) {
  .game-area {
    padding-bottom: 300px; /* Espacio para el teclado virtual */
  }
  
  body {
    height: 100vh;
    height: 100dvh; /* Altura dinámica para móviles */
  }
}

/* Estilos para ComboMessage - independiente del CentralMessage */
.combo-message {
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 110; /* Mayor que central-message para que aparezca encima */
  pointer-events: none;
  animation: comboSlideIn 0.3s ease-out;
}

@keyframes comboSlideIn {
  0% {
    transform: translate(-50%, -70%) scale(0.5);
    opacity: 0;
  }
  50% {
    transform: translate(-50%, -45%) scale(1.2);
    opacity: 0.8;
  }
  100% {
    transform: translate(-50%, -50%) scale(1);
    opacity: 1;
  }
}

.combo-message-content {
  background: linear-gradient(45deg, rgba(255, 215, 0, 0.9), rgba(255, 140, 0, 0.9));
  border: 4px solid #ffd700;
  border-radius: 25px;
  padding: 25px 40px;
  text-align: center;
  box-shadow: 
    0 0 50px rgba(255, 215, 0, 0.8),
    inset 0 0 20px rgba(255, 255, 255, 0.3);
  animation: comboGlow 1s ease-in-out infinite alternate;
  color: #000;
  font-family: "Press Start 2P", monospace;
  font-size: 40px;
  font-weight: normal;
  text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.5);
  transform: rotate(-2deg);
}

@keyframes comboGlow {
  0% {
    box-shadow: 
      0 0 50px rgba(255, 215, 0, 0.8),
      inset 0 0 20px rgba(255, 255, 255, 0.3);
    border-color: #ffd700;
    transform: rotate(-2deg) scale(1);
  }
  100% {
    box-shadow: 
      0 0 80px rgba(255, 215, 0, 1),
      inset 0 0 30px rgba(255, 255, 255, 0.5);
    border-color: #ffed4e;
    transform: rotate(2deg) scale(1.05);
  }
}

/* Estilos para OrderMessage - mensaje de orden perfecto */
.order-message {
  position: fixed;
  top: 35%; /* Posición más arriba para no pisarse con combo */
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 115; /* Ligeramente mayor que combo para aparecer encima */
  pointer-events: none;
  animation: orderSlideIn 0.4s ease-out;
}

@keyframes orderSlideIn {
  0% {
    transform: translate(-50%, -20%) scale(0.3);
    opacity: 0;
  }
  50% {
    transform: translate(-50%, -30%) scale(1.3);
    opacity: 0.9;
  }
  100% {
    transform: translate(-50%, -50%) scale(1);
    opacity: 1;
  }
}

.order-message-content {
  background: linear-gradient(45deg, rgba(0, 255, 127, 0.9), rgba(0, 191, 255, 0.9));
  border: 4px solid #00ff7f;
  border-radius: 20px;
  padding: 20px 35px;
  text-align: center;
  box-shadow: 
    0 0 40px rgba(0, 255, 127, 0.8),
    inset 0 0 15px rgba(255, 255, 255, 0.4);
  animation: orderGlow 0.8s ease-in-out infinite alternate;
  color: #000;
  font-family: "Press Start 2P", monospace;
  font-size: 32px;
  font-weight: normal;
  text-shadow: 1px 1px 3px rgba(0, 0, 0, 0.6);
  transform: rotate(1deg);
}

@keyframes orderGlow {
  0% {
    box-shadow: 
      0 0 40px rgba(0, 255, 127, 0.8),
      inset 0 0 15px rgba(255, 255, 255, 0.4);
    border-color: #00ff7f;
    transform: rotate(1deg) scale(1);
  }
  100% {
    box-shadow: 
      0 0 60px rgba(0, 255, 127, 1),
      inset 0 0 25px rgba(255, 255, 255, 0.6);
    border-color: #40ffbf;
    transform: rotate(-1deg) scale(1.02);
  }
}

/* Estilos para MissileLetterComponent - Misiles con estelas de fuego */
.missile-letter {
  position: absolute;
  pointer-events: none;
  transform-origin: center;
}

.missile-body {
  position: relative;
  z-index: 5;
  width: 60px;
  height: 60px;
  display: flex;
  align-items: center;
  justify-content: center;
}

.missile-letter.highlighted .missile-body {
  box-shadow: 
    0 0 25px rgba(255, 255, 0, 0.8),
    inset 0 0 15px rgba(255, 255, 0, 0.3);
  transform: scale(1.1);
}

.missile-letter-image {
  transition: all 0.3s ease;
  border-radius: 5px;
}

.missile-letter.highlighted .missile-letter-image {
  filter: drop-shadow(0 0 20px #ffff00) brightness(1.3);
  transform: scale(1.1);
}

.missile-letter-text {
  font-family: "Press Start 2P", monospace;
}

.missile-letter.highlighted .missile-letter-text {
  text-shadow: 0 0 20px #ffff00;
  transform: scale(1.1);
}

/* Estela de fuego - ahora hacia arriba del misil */
.fire-trail {
  position: absolute;
  top: -90%; /* Arriba del misil en lugar de abajo */
  left: 50%;
  transform: translateX(-50%);
  width: 30px;
  height: 80px;
  z-index: 3;
  pointer-events: none;
}

.fire-particle {
  position: absolute;
  border-radius: 50%;
  animation-iteration-count: infinite;
  animation-timing-function: ease-out;
  filter: blur(4px); /* Aumentar blur general */
  box-shadow: 0 0 8px currentColor, 0 4px 12px rgba(255, 100, 0, 0.6); /* Blur adicional en dirección Y */
}

.fire-1 {
  width: 16px;
  height: 16px;
  background: radial-gradient(circle, #ff4500 0%, #ff6600 50%, transparent 100%);
  left: 7px;
  top: 64px; /* Más abajo en el trail (más cerca del misil) */
  animation: fireParticle1 0.4s infinite;
}

.fire-2 {
  width: 14px;
  height: 14px;
  background: radial-gradient(circle, #ff6600 0%, #ff8800 50%, transparent 100%);
  left: 8px;
  top: 52px;
  animation: fireParticle2 0.5s infinite;
}

.fire-3 {
  width: 12px;
  height: 12px;
  background: radial-gradient(circle, #ff8800 0%, #ffaa00 50%, transparent 100%);
  left: 9px;
  top: 40px;
  animation: fireParticle3 0.6s infinite;
}

.fire-4 {
  width: 10px;
  height: 10px;
  background: radial-gradient(circle, #ffaa00 0%, #ffcc00 50%, transparent 100%);
  left: 10px;
  top: 28px;
  animation: fireParticle4 0.8s infinite;
}

.fire-5 {
  width: 8px;
  height: 8px;
  background: radial-gradient(circle, #ffcc00 0%, #ffee00 50%, transparent 100%);
  left: 11px;
  top: 16px;
  animation: fireParticle5 1s infinite;
}

/* Animaciones invertidas - ahora van hacia arriba */
@keyframes fireParticle1 {
  0% {
    opacity: 1;
    transform: translateY(0) scale(1);
  }
  50% {
    opacity: 0.8;
    transform: translateY(-8px) scale(1.2);
  }
  100% {
    opacity: 0;
    transform: translateY(-16px) scale(0.8);
  }
}

@keyframes fireParticle2 {
  0% {
    opacity: 0.9;
    transform: translateY(0) scale(1);
  }
  50% {
    opacity: 0.7;
    transform: translateY(-10px) scale(1.1);
  }
  100% {
    opacity: 0;
    transform: translateY(-20px) scale(0.7);
  }
}

@keyframes fireParticle3 {
  0% {
    opacity: 0.8;
    transform: translateY(0) scale(1);
  }
  50% {
    opacity: 0.6;
    transform: translateY(-12px) scale(1);
  }
  100% {
    opacity: 0;
    transform: translateY(-24px) scale(0.6);
  }
}

@keyframes fireParticle4 {
  0% {
    opacity: 0.7;
    transform: translateY(0) scale(1);
  }
  50% {
    opacity: 0.5;
    transform: translateY(-14px) scale(0.9);
  }
  100% {
    opacity: 0;
    transform: translateY(-28px) scale(0.5);
  }
}

@keyframes fireParticle5 {
  0% {
    opacity: 0.6;
    transform: translateY(0) scale(1);
  }
  50% {
    opacity: 0.4;
    transform: translateY(-16px) scale(0.8);
  }
  100% {
    opacity: 0;
    transform: translateY(-32px) scale(0.4);
  }
}

/* Información del sector en la parte superior */
.sector-info {
  margin: 10px;
  z-index: 10;
}

.sector-panel {
  border: 3px solid #00ffff;
  border-radius: 20px;
  padding: 15px 30px;
  text-align: center;
  box-shadow: 
    0 0 30px rgba(0, 255, 255, 0.5),
    inset 0 0 20px rgba(0, 255, 255, 0.1);
  backdrop-filter: blur(4px);
  animation: sectorGlow 3s ease-in-out infinite alternate;
}

.sector-label {
  font-size: 8px;
  color: #ffaa00;
  text-shadow: 0 0 5px #ffaa00;
  margin-bottom: 8px;
  font-family: "Press Start 2P", monospace;
  letter-spacing: 2px;
}

.sector-name {
  font-size: 18px;
  margin-bottom: 8px;
  color: #ffaa00;
  text-shadow: 0 0 15px #ffaa00;
  font-family: "Press Start 2P", monospace;
  font-weight: normal;
}

.sector-description {
  font-size: 12px;
  color: #00ffff;
  opacity: 0.9;
  font-family: "Press Start 2P", monospace;
  font-weight: normal;
}

@keyframes sectorGlow {
  0% {
    box-shadow: 0 0 30px rgba(0, 255, 255, 0.5), inset 0 0 20px rgba(0, 255, 255, 0.1);
    border-color: #00ffff;
  }
  100% {
    box-shadow: 0 0 50px rgba(0, 255, 255, 0.7), inset 0 0 30px rgba(0, 255, 255, 0.2);
    border-color: #66ffff;
  }
}

/* Score display - números más pequeños */
.score-display {
  background: rgba(0, 0, 0, 0.7);
  border: 3px solid #00ffff;
  padding: 15px 25px;
  border-radius: 20px;
  text-align: center;
  min-width: 250px;
  box-shadow: 
    0 0 25px rgba(0, 255, 255, 0.4),
    inset 0 0 15px rgba(0, 255, 255, 0.1);
}

.score-display .instrument-label {
  font-size: 10px;
  color: #ffaa00;
  text-shadow: 0 0 8px #ffaa00;
  margin-bottom: 8px;
  font-family: "Press Start 2P", monospace;
  letter-spacing: 2px;
}

.score-value {
  font-size: 28px;
  color: #00ffff;
  text-shadow: 0 0 20px #00ffff;
  font-family: "Press Start 2P", monospace;
  font-weight: normal;
  letter-spacing: 3px;
  animation: scoreAnimation 0.5s ease-out;
}

/* Lives display con naves en miniatura */
.lives-display {
  background: rgba(0, 0, 0, 0.7);
  border: 2px solid #00ffff;
  padding: 15px 20px;
  border-radius: 15px;
  text-align: center;
  box-shadow: 
    0 0 15px rgba(0, 255, 255, 0.3),
    inset 0 0 10px rgba(0, 255, 255, 0.1);
}

.lives-display .instrument-label {
  font-size: 10px;
  color: #ffaa00;
  text-shadow: 0 0 5px #ffaa00;
  margin-bottom: 8px;
  font-family: "Press Start 2P", monospace;
  letter-spacing: 2px;
}

.lives-ships {
  display: flex;
  justify-content: center;
  gap: 8px;
  align-items: center;
}

.life-ship {
  width: 30px;
  height: 30px;
  display: flex;
  align-items: center;
  justify-content: center;
}

.mini-ship {
  width: 24px;
  height: 24px;
  background-image: url(../assets/images/nave.png);
  background-size: contain;
  background-repeat: no-repeat;
  background-position: center;
  filter: drop-shadow(0 0 8px #00ffff);
  animation: miniShipGlow 2s ease-in-out infinite alternate;
}

@keyframes miniShipGlow {
  0% {
    filter: drop-shadow(0 0 8px #00ffff);
    transform: scale(1);
  }
  100% {
    filter: drop-shadow(0 0 12px #00ffff);
    transform: scale(1.05);
  }
}

/* Mute toggle integrado */
.mute-toggle {
  background: rgba(0, 0, 0, 0.7);
  border: 2px solid #00ffff;
  padding: 15px 20px;
  border-radius: 15px;
  text-align: center;
  cursor: pointer;
  transition: all 0.3s ease;
  box-shadow: 
    0 0 15px rgba(0, 255, 255, 0.3),
    inset 0 0 10px rgba(0, 255, 255, 0.1);
  min-width: 100px;
}

.mute-toggle .instrument-label {
  font-size: 8px;
  color: #ffaa00;
  text-shadow: 0 0 5px #ffaa00;
  margin-bottom: 8px;
  font-family: "Press Start 2P", monospace;
  letter-spacing: 1px;
}

.mute-icon {
  display: flex;
  justify-content: center;
  align-items: center;
}

.mute-toggle.unmuted {
  color: #00ffff;
  border-color: #00ffff;
}

.mute-toggle.unmuted svg {
  filter: drop-shadow(0 0 5px #00ffff);
}

.mute-toggle.muted {
  color: #ff0066;
  border-color: #ff0066;
  box-shadow: 0 0 15px rgba(255, 0, 102, 0.3);
}

.mute-toggle.muted svg {
  filter: drop-shadow(0 0 5px #ff0066);
}

.mute-toggle:hover {
  transform: scale(1.05);
  box-shadow: 0 0 20px rgba(0, 255, 255, 0.5);
}

.mute-toggle:active {
  transform: scale(0.95);
}

.game-area {
  position: relative;
  width: 100%;
  height: 100%;
  overflow: hidden;
  display: flex;
  justify-content: center;
}

.cannon {
  position: absolute;
  bottom: 150px;
  left: 50%;
  transform: translateX(-50%);
  width: 80px;
  height: 100px;
  z-index: 5;
  transition: transform 0.05s ease-out;
  animation: shipFloating 3s ease-in-out infinite;
}

@keyframes shipFloating {
  0%, 100% {
    transform: translateX(-50%) translateY(0px);
  }
  50% {
    transform: translateX(-50%) translateY(-5px);
  }
}

.cannon-base {
  position: absolute;
  left: -40px;
  width: 160px;
  height: 160px;
  transform: scale(0.5);
  z-index: 7;
  background-image: url(../assets/images/nave.png);
}

.cannon-barrel {
  position: absolute;
  left: 50%;
  top: 42px;
  transform: translateX(-50%);
  width: 8px;
  height: 28px;
  background: linear-gradient(to top, #444, #777);
  border: 2px solid #1900ff;
  box-shadow: 0 0 15px rgba(0, 255, 255, 0.5);
  animation: cannonGlow 2s ease-in-out infinite alternate;
  z-index: 5;
  transform-origin: center bottom;
}

.cannon-base::before {
  width: 30px;
  height: 60px;
  left: 50%;
  top: 30px;
  transform: translateX(-50%);
  border-radius: 100%;
  background: radial-gradient(#ffffff, #8c00ff, #8c00ff);
  border: 2px solid #1900ff;
  content: "";
  display: block;
  position: relative;
  z-index: 6;
}

@keyframes cannonGlow {
  0% {
    box-shadow: 0 0 15px rgba(0, 255, 255, 0.5);
  }
  100% {
    box-shadow: 0 0 25px rgba(0, 255, 255, 0.8);
  }
}

.cannon.reloading .cannon-barrel {
  border-color: #ff0066;
  box-shadow: 0 0 15px rgba(255, 136, 136, 0.5);
  animation: reloadPulse 0.3s infinite;
}

@keyframes reloadPulse {
  0%,
  100% {
    transform: translateX(-50%) scale(1);
  }
  50% {
    transform: translateX(-50%) scale(1.05);
  }
}

.falling-letter {
  position: absolute;
  width: 60px;
  height: 60px;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #00ffff;
  font-family: "Press Start 2P", monospace;
  font-size: 16px;
  font-weight: normal;
  z-index: 3;
  text-shadow: 0 0 8px currentColor;
}

@keyframes letterFall {
  from {
    transform: translateY(-100px);
  }
  to {
    transform: translateY(calc(100vh + 100px));
  }
}

/* Estilos para imágenes de letras de la fila del medio */
.falling-letter img {
  transition: all 0.3s ease;
  border-radius: 5px;
  animation: letterImageGlow 2s ease-in-out infinite alternate;
}

@keyframes letterImageGlow {
  0% {
    filter: drop-shadow(0 0 8px currentColor) brightness(1);
    transform: scale(1);
  }
  100% {
    filter: drop-shadow(0 0 15px currentColor) brightness(1.1);
    transform: scale(1.02);
  }
}

/* Efecto especial cuando se presiona una tecla para letras con imagen */
.falling-letter.highlighted img {
  filter: drop-shadow(0 0 20px #ffff00) brightness(1.3);
  transform: scale(1.1);
  animation: highlightedImagePulse 0.3s ease-in-out infinite alternate;
}

@keyframes highlightedImagePulse {
  0% {
    filter: drop-shadow(0 0 20px #ffff00) brightness(1.3);
  }
  100% {
    filter: drop-shadow(0 0 30px #ffaa00) brightness(1.5);
  }
}

.bullet {
  position: fixed;
  width: 8px;
  height: 20px;
  background: linear-gradient(to top, #ffff00, #ff6600);
  border-radius: 50% 50% 0 0;
  box-shadow: 0 0 15px rgba(255, 255, 0, 0.8);
  z-index: 4;
  animation: bulletTrail 0.1s ease-out infinite;
  pointer-events: none;
}

@keyframes bulletTrail {
  0% {
    box-shadow: 0 0 15px rgba(255, 255, 0, 0.8);
  }
  100% {
    box-shadow: 0 -10px 20px rgba(255, 255, 0, 0.4),
      0 0 15px rgba(255, 255, 0, 0.8);
  }
}

.explosion {
  /* Eliminado - solo usamos partículas ahora */
  display: none;
}

@keyframes explode {
  /* Eliminado - solo usamos partículas ahora */
}

.particle {
  position: absolute;
  width: 10px;
  height: 10px;
  background: #00ffff;
  pointer-events: none;
  z-index: 5;
}

.instructions {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  text-align: center;
  color: #00ffff;
  z-index: 10;
  background: rgba(0, 0, 0, 0.8);
  padding: 30px;
  border-radius: 20px;
  border: 2px solid #00ffff;
  box-shadow: 0 0 30px rgba(0, 255, 255, 0.3);
  animation: instructionsGlow 2s ease-in-out infinite alternate;
}

@keyframes instructionsGlow {
  0% {
    box-shadow: 0 0 30px rgba(0, 255, 255, 0.3);
  }
  100% {
    box-shadow: 0 0 40px rgba(0, 255, 255, 0.6);
  }
}

.instructions h2 {
  font-size: 28px;
  margin-bottom: 20px;
  text-shadow: 0 0 15px #00ffff;
}

.instructions p {
  font-size: 16px;
  margin-bottom: 10px;
  line-height: 1.5;
}

.start-btn {
  background: linear-gradient(45deg, #00ffff, #0066ff);
  border: none;
  padding: 15px 30px;
  border-radius: 10px;
  color: white;
  font-family: "Press Start 2P", monospace;
  font-size: 12px;
  font-weight: normal;
  cursor: pointer;
  margin-top: 20px;
  transition: all 0.3s ease;
  box-shadow: 0 0 20px rgba(0, 255, 255, 0.4);
  text-shadow: 0 0 5px rgba(0, 0, 0, 0.8);
}

.start-btn:hover {
  transform: scale(1.05);
  box-shadow: 0 0 30px rgba(0, 255, 255, 0.7);
}

.start-btn:active {
  transform: scale(0.95);
}

.hidden {
  display: none;
}

.game-over {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  text-align: center;
  color: #ff0066;
  z-index: 10;
  background: rgba(0, 0, 0, 0.9);
  padding: 40px;
  border-radius: 20px;
  border: 3px solid #ff0066;
  box-shadow: 0 0 40px rgba(255, 0, 102, 0.5);
}

.game-over h2 {
  font-size: 36px;
  margin-bottom: 20px;
  text-shadow: 0 0 20px #ff0066;
  animation: gameOverPulse 1s ease-in-out infinite alternate;
}

.game-over p {
  font-size: 18px;
  margin-bottom: 30px;
  color: #ffffff;
}

.game-over p span {
  color: #ffaa00;
  text-shadow: 0 0 15px #ffaa00;
  font-weight: bold;
}

.game-over-buttons {
  display: flex;
  flex-direction: column;
  gap: 20px;
  align-items: center;
}

.continue-btn {
  background: linear-gradient(45deg, #00ff00, #00aa00);
  border: none;
  padding: 15px 30px;
  border-radius: 10px;
  color: white;
  font-family: "Press Start 2P", monospace;
  font-size: 12px;
  font-weight: normal;
  cursor: pointer;
  transition: all 0.3s ease;
  box-shadow: 0 0 20px rgba(0, 255, 0, 0.4);
  text-shadow: 0 0 5px rgba(0, 0, 0, 0.8);
}

.continue-btn:hover {
  transform: scale(1.05);
  box-shadow: 0 0 30px rgba(0, 255, 0, 0.7);
  background: linear-gradient(45deg, #00ff44, #00cc00);
}

.continue-btn:active {
  transform: scale(0.95);
}

@keyframes gameOverPulse {
  0% {
    transform: scale(1);
  }
  100% {
    transform: scale(1.1);
  }
}

/* Estilos para HandMap */
.hand-maps-container {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
  pointer-events: none;
  z-index: 1;
  padding: 20px;
}

/* Ocultar las manos en dispositivos móviles */
@media (max-width: 768px) {
  .hand-maps-container {
    display: none;
  }
}

.hand-map {
  padding: 15px;
  width: 180px;
  color: #00ffff;
  font-family: "Press Start 2P", monospace;
}

.hand-map-left {
  margin-top: 120px;
}

.hand-map-right {
  margin-top: 120px;
}

@keyframes handMapGlow {
  0% {
    box-shadow: 0 0 20px rgba(0, 255, 255, 0.3);
  }
  100% {
    box-shadow: 0 0 30px rgba(0, 255, 255, 0.5);
  }
}


.hand-visual {
  display: flex;
  justify-content: center;
  margin-bottom: 10px;
}

.hand-visual svg {
  filter: drop-shadow(0 0 5px rgba(0, 255, 255, 0.5));
  width: 100px;
  height: 140px;
}

.palm {
  fill: rgba(0, 255, 255, 0.2);
  stroke: #00ffff;
  stroke-width: 2;
}

.finger {
  fill: rgba(0, 255, 255, 0.3);
  stroke: #00ffff;
  stroke-width: 1.5;
  transition: all 0.3s ease;
}

.finger.highlighted {
  fill: rgba(255, 255, 0, 0.7);
  stroke: #ffff00;
  stroke-width: 3;
  filter: drop-shadow(0 0 10px #ffff00);
  animation: fingerPulse 0.6s ease-in-out infinite alternate;
}

@keyframes fingerPulse {
  0% {
    fill: rgba(255, 255, 0, 0.7);
    stroke: #ffff00;
  }
  100% {
    fill: rgba(255, 165, 0, 0.9);
    stroke: #ffa500;
  }
}

.finger-indicator {
  text-align: center;
  margin-bottom: 10px;
  padding: 6px;
  background: rgba(255, 255, 0, 0.15);
  border: 1px solid #ffff00;
  border-radius: 8px;
  animation: indicatorGlow 1s ease-in-out infinite alternate;
}

@keyframes indicatorGlow {
  0% {
    background: rgba(255, 255, 0, 0.15);
    border-color: #ffff00;
  }
  100% {
    background: rgba(255, 255, 0, 0.25);
    border-color: #ffa500;
  }
}

.finger-name {
  display: block;
  font-weight: bold;
  font-size: 12px;
  color: #ffff00;
  text-shadow: 0 0 8px #ffff00;
}

.key-name {
  display: block;
  font-size: 16px;
  font-weight: bold;
  color: #ffa500;
  text-shadow: 0 0 8px #ffa500;
  margin-top: 2px;
}

.key-list {
  max-height: 150px;
  overflow-y: auto;
  scrollbar-width: thin;
  scrollbar-color: #00ffff rgba(0, 0, 0, 0.3);
}

.key-list::-webkit-scrollbar {
  width: 4px;
}

.key-list::-webkit-scrollbar-track {
  background: rgba(0, 0, 0, 0.3);
  border-radius: 2px;
}

.key-list::-webkit-scrollbar-thumb {
  background: #00ffff;
  border-radius: 2px;
}

.key-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 3px 6px;
  margin-bottom: 1px;
  border-radius: 4px;
  transition: all 0.3s ease;
  font-size: 11px;
}

.key-item:hover {
  background: rgba(0, 255, 255, 0.1);
}

.key-item.active {
  background: rgba(255, 255, 0, 0.25);
  border: 1px solid #ffff00;
  animation: keyItemPulse 0.5s ease-in-out infinite alternate;
  transform: scale(1.02);
}

@keyframes keyItemPulse {
  0% {
    background: rgba(255, 255, 0, 0.25);
    box-shadow: 0 0 5px rgba(255, 255, 0, 0.3);
  }
  100% {
    background: rgba(255, 255, 0, 0.35);
    box-shadow: 0 0 8px rgba(255, 255, 0, 0.5);
  }
}

.key {
  font-weight: bold;
  color: #00ffff;
  background: rgba(0, 255, 255, 0.2);
  padding: 2px 5px;
  border-radius: 3px;
  min-width: 18px;
  text-align: center;
  font-size: 11px;
}

.key-item.active .key {
  color: #ffff00;
  background: rgba(255, 255, 0, 0.4);
  text-shadow: 0 0 5px #ffff00;
  font-weight: bold;
}

.finger-label {
  color: #888;
  font-size: 9px;
}

.key-item.active .finger-label {
  color: #ffa500;
  font-weight: bold;
  text-shadow: 0 0 3px #ffa500;
}

/* Estilos para CentralMessage */
.central-message {
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 100;
  pointer-events: none;
}

.central-message-content {
  background: rgba(0, 0, 0, 0.95);
  border: 3px solid #00ffff;
  border-radius: 20px;
  padding: 30px 50px;
  text-align: center;
  box-shadow: 0 0 50px rgba(0, 255, 255, 0.8);
  animation: centralMessageGlow 2s ease-in-out infinite alternate;
  backdrop-filter: blur(15px);
}

@keyframes centralMessageGlow {
  0% {
    box-shadow: 0 0 50px rgba(0, 255, 255, 0.8);
    border-color: #00ffff;
  }
  100% {
    box-shadow: 0 0 80px rgba(0, 255, 255, 1);
    border-color: #66ffff;
  }
}

.status-message {
  color: #00ffff;
  font-family: "Press Start 2P", monospace;
  font-size: 16px;
  font-weight: normal;
  text-shadow: 0 0 20px #00ffff;
  animation: statusPulse 1.5s ease-in-out infinite alternate;
}

@keyframes statusPulse {
  0% {
    transform: scale(1);
    text-shadow: 0 0 20px #00ffff;
  }
  100% {
    transform: scale(1.05);
    text-shadow: 0 0 30px #00ffff;
  }
}

.countdown {
  color: #ffff00;
  font-family: "Press Start 2P", monospace;
}

.countdown-number {
  font-size: 48px;
  font-weight: normal;
  text-shadow: 0 0 30px #ffff00;
  animation: countdownPulse 1s ease-in-out;
  margin-bottom: 10px;
}

@keyframes countdownPulse {
  0% {
    transform: scale(0.5);
    opacity: 0;
    text-shadow: 0 0 30px #ffff00;
  }
  50% {
    transform: scale(1.2);
    opacity: 1;
    text-shadow: 0 0 50px #ffff00, 0 0 80px #ffa500;
  }
  100% {
    transform: scale(1);
    opacity: 1;
    text-shadow: 0 0 30px #ffff00;
  }
}

.countdown-text {
  font-size: 20px;
  font-weight: bold;
  color: #ffa500;
  text-shadow: 0 0 15px #ffa500;
  animation: countdownTextGlow 1s ease-in-out infinite alternate;
}

@keyframes countdownTextGlow {
  0% {
    color: #ffa500;
    text-shadow: 0 0 15px #ffa500;
  }
  100% {
    color: #ffff00;
    text-shadow: 0 0 20px #ffff00;
  }
}

/* Modificar el HUD para que no muestre el status cuando hay mensaje central */
.hud .status.hidden {
  display: none;
}

/* Estilos para el botón de sonido */
.sound-toggle {
  position: fixed;
  top: 20px;
  right: 20px;
  width: 50px;
  height: 50px;
  background: rgba(0, 0, 0, 0.8);
  border: 2px solid #00ffff;
  border-radius: 10px;
  display: flex;
  align-items: center;
  justify-content: center;
  cursor: pointer;
  transition: all 0.3s ease;
  z-index: 1000;
  font-family: "Orbitron", monospace;
  box-shadow: 0 0 20px rgba(0, 255, 255, 0.3);
  animation: soundButtonGlow 2s ease-in-out infinite alternate;
}

@keyframes soundButtonGlow {
  0% {
    box-shadow: 0 0 20px rgba(0, 255, 255, 0.3);
    border-color: #00ffff;
  }
  100% {
    box-shadow: 0 0 30px rgba(0, 255, 255, 0.5);
    border-color: #66ffff;
  }
}

.sound-toggle:hover {
  transform: scale(1.1);
  box-shadow: 0 0 30px rgba(0, 255, 255, 0.6);
}

.sound-toggle:active {
  transform: scale(0.95);
}

.sound-toggle.unmuted {
  color: #00ffff;
  text-shadow: 0 0 10px #00ffff;
}

.sound-toggle.unmuted svg {
  filter: drop-shadow(0 0 5px #00ffff);
}

.sound-toggle.muted {
  color: #ff0066;
  border-color: #ff0066;
  text-shadow: 0 0 10px #ff0066;
  box-shadow: 0 0 20px rgba(255, 0, 102, 0.3);
  animation: mutedButtonGlow 2s ease-in-out infinite alternate;
}

@keyframes mutedButtonGlow {
  0% {
    box-shadow: 0 0 20px rgba(255, 0, 102, 0.3);
    border-color: #ff0066;
  }
  100% {
    box-shadow: 0 0 30px rgba(255, 0, 102, 0.5);
    border-color: #ff6699;
  }
}

.sound-toggle.muted svg {
  filter: drop-shadow(0 0 5px #ff0066);
}

.sound-toggle.muted:hover {
  box-shadow: 0 0 30px rgba(255, 0, 102, 0.6);
}

/* Estilos para el campo de fuerza */
.force-field {
  pointer-events: none;
  transform: translate(-50%, -50%);
  animation: forceFieldGrow 0.3s ease-out forwards, forceFieldPulse 0.5s ease-in-out 0.3s infinite alternate;
}

@keyframes forceFieldGrow {
  0% {
    transform: translate(-50%, -50%) scale(0);
    opacity: 0;
  }
  100% {
    transform: translate(-50%, -50%) scale(1);
    opacity: 1;
  }
}

@keyframes forceFieldPulse {
  0% {
    transform: translate(-50%, -50%) scale(1);
    box-shadow: 0 0 50px #ff0000, inset 0 0 50px rgba(0, 255, 255, 0.2);
    border-color: #ff0000;
  }
  100% {
    transform: translate(-50%, -50%) scale(1.05);
    box-shadow: 0 0 80px #ff0000, inset 0 0 80px rgba(255, 0, 0, 0.4);
    border-color: #ff0000;
  }
}

/* Estilos para meteoritos */
.meteorite {
  pointer-events: none;
  animation: meteoriteGlow 1s ease-in-out infinite alternate;
  position: relative;
}

/* Punto central para debug de colisiones - más visible */
.meteorite > div:first-child {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 6px !important;
  height: 6px !important;
  background: #ff0000 !important;
  border-radius: 50% !important;
  transform: translate(-50%, -50%) !important;
  z-index: 20 !important;
  box-shadow: 0 0 8px #ff0000 !important;
}

@keyframes meteoriteGlow {
  0% {
    box-shadow: 0 0 20px #ff4500, inset 0 0 10px rgba(255, 69, 0, 0.5);
    border-color: #ff6600;
  }
  100% {
    box-shadow: 0 0 30px #ff4500, inset 0 0 15px rgba(255, 69, 0, 0.7);
    border-color: #ff8800;  
  }
}

/* Estilos para la sección de inicio */
.instructions strong {
  color: #ffaa00;
  text-shadow: 0 0 10px #ffaa00;
}

.start-section {
  border-top: 1px solid rgba(0, 255, 255, 0.3);
  padding-top: 20px;
  margin-top: 20px;
}

.start-instruction {
  font-size: 18px;
  margin-bottom: 15px;
  color: #ffffff;
}

.start-instruction strong {
  color: #00ff00;
  text-shadow: 0 0 10px #00ff00;
  font-family: "Courier New", monospace;
  background: rgba(0, 255, 0, 0.1);
  padding: 2px 6px;
  border-radius: 4px;
  animation: enterKeyGlow 2s ease-in-out infinite alternate;
}

@keyframes enterKeyGlow {
  0% {
    box-shadow: 0 0 5px rgba(0, 255, 0, 0.3);
    background: rgba(0, 255, 0, 0.1);
  }
  100% {
    box-shadow: 0 0 15px rgba(0, 255, 0, 0.5);
    background: rgba(0, 255, 0, 0.2);
  }
}

.status.force-field-active {
  border-color: #ffaa00;
  color: #ffaa00;
  text-shadow: 0 0 10px #ffaa00;
  box-shadow: 0 0 20px rgba(255, 170, 0, 0.3);
  animation: forceFieldStatusPulse 1s ease-in-out infinite alternate;
}

@keyframes forceFieldStatusPulse {
  0% {
    opacity: 0.8;
    transform: scale(1);
  }
  100% {
    opacity: 1;
    transform: scale(1.02);
  }
}

.instrument-panel {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  max-width: 700px;
  gap: 30px;
}

.instrument-label {
  font-size: 12px;
  color: #ffaa00;
  text-shadow: 0 0 8px #ffaa00;
  margin-bottom: 10px;
  font-family: "Press Start 2P", monospace;
  letter-spacing: 3px;
}

.instrument-value {
  font-size: 12px;
  color: #00ffff;
  text-shadow: 0 0 8px #00ffff;
  text-align: center;
  font-weight: normal;
}

.status-indicator {
  background: rgba(0, 0, 0, 0.7);
  border: 2px solid #00ffff;
  padding: 8px 15px;
  border-radius: 15px;
  color: #00ffff;
  font-family: "Press Start 2P", monospace;
  font-size: 10px;
  font-weight: normal;
  text-shadow: 0 0 8px #00ffff;
  box-shadow: 
    0 0 15px rgba(0, 255, 255, 0.3),
    inset 0 0 10px rgba(0, 255, 255, 0.1);
  margin: 0 10px;
  min-width: 80px;
  text-align: center;
  position: relative;
}

.status-light {
  font-size: 16px;
  text-align: center;
  margin-top: 4px;
}

.status-light.ready {
  color: #00ff00;
  text-shadow: 0 0 10px #00ff00;
}

.status-light.penalty {
  color: #ff0066;
  text-shadow: 0 0 10px #ff0066;
  animation: statusAlert 0.5s infinite alternate;
}

@keyframes statusAlert {
  0% { opacity: 0.7; }
  100% { opacity: 1; }
}

/* Ajustar el área de juego en móviles para el teclado */
@media (max-width: 768px) {
  .game-area {
    padding-bottom: 300px; /* Espacio para el teclado virtual */
  }
  
  body {
    height: 100vh;
    height: 100dvh; /* Altura dinámica para móviles */
  }
}

/* Estilos para ComboMessage - independiente del CentralMessage */
.combo-message {
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 110; /* Mayor que central-message para que aparezca encima */
  pointer-events: none;
  animation: comboSlideIn 0.3s ease-out;
}

@keyframes comboSlideIn {
  0% {
    transform: translate(-50%, -70%) scale(0.5);
    opacity: 0;
  }
  50% {
    transform: translate(-50%, -45%) scale(1.2);
    opacity: 0.8;
  }
  100% {
    transform: translate(-50%, -50%) scale(1);
    opacity: 1;
  }
}

.combo-message-content {
  background: linear-gradient(45deg, rgba(255, 215, 0, 0.9), rgba(255, 140, 0, 0.9));
  border: 4px solid #ffd700;
  border-radius: 25px;
  padding: 25px 40px;
  text-align: center;
  box-shadow: 
    0 0 50px rgba(255, 215, 0, 0.8),
    inset 0 0 20px rgba(255, 255, 255, 0.3);
  animation: comboGlow 1s ease-in-out infinite alternate;
  color: #000;
  font-family: "Press Start 2P", monospace;
  font-size: 40px;
  font-weight: normal;
  text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.5);
  transform: rotate(-2deg);
}

@keyframes comboGlow {
  0% {
    box-shadow: 
      0 0 50px rgba(255, 215, 0, 0.8),
      inset 0 0 20px rgba(255, 255, 255, 0.3);
    border-color: #ffd700;
    transform: rotate(-2deg) scale(1);
  }
  100% {
    box-shadow: 
      0 0 80px rgba(255, 215, 0, 1),
      inset 0 0 30px rgba(255, 255, 255, 0.5);
    border-color: #ffed4e;
    transform: rotate(2deg) scale(1.05);
  }
}

/* Estilos para OrderMessage - mensaje de orden perfecto */
.order-message {
  position: fixed;
  top: 35%; /* Posición más arriba para no pisarse con combo */
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 115; /* Ligeramente mayor que combo para aparecer encima */
  pointer-events: none;
  animation: orderSlideIn 0.4s ease-out;
}

@keyframes orderSlideIn {
  0% {
    transform: translate(-50%, -20%) scale(0.3);
    opacity: 0;
  }
  50% {
    transform: translate(-50%, -30%) scale(1.3);
    opacity: 0.9;
  }
  100% {
    transform: translate(-50%, -50%) scale(1);
    opacity: 1;
  }
}

.order-message-content {
  background: linear-gradient(45deg, rgba(0, 255, 127, 0.9), rgba(0, 191, 255, 0.9));
  border: 4px solid #00ff7f;
  border-radius: 20px;
  padding: 20px 35px;
  text-align: center;
  box-shadow: 
    0 0 40px rgba(0, 255, 127, 0.8),
    inset 0 0 15px rgba(255, 255, 255, 0.4);
  animation: orderGlow 0.8s ease-in-out infinite alternate;
  color: #000;
  font-family: "Press Start 2P", monospace;
  font-size: 32px;
  font-weight: normal;
  text-shadow: 1px 1px 3px rgba(0, 0, 0, 0.6);
  transform: rotate(1deg);
}

@keyframes orderGlow {
  0% {
    box-shadow: 
      0 0 40px rgba(0, 255, 127, 0.8),
      inset 0 0 15px rgba(255, 255, 255, 0.4);
    border-color: #00ff7f;
    transform: rotate(1deg) scale(1);
  }
  100% {
    box-shadow: 
      0 0 60px rgba(0, 255, 127, 1),
      inset 0 0 25px rgba(255, 255, 255, 0.6);
    border-color: #40ffbf;
    transform: rotate(-1deg) scale(1.02);
  }
}

/* Estilos para MissileLetterComponent - Misiles con estelas de fuego */
.missile-letter {
  position: absolute;
  pointer-events: none;
  transform-origin: center;
}

.missile-body {
  position: relative;
  z-index: 5;
  width: 60px;
  height: 60px;
  display: flex;
  align-items: center;
  justify-content: center;
}

.missile-letter.highlighted .missile-body {
  box-shadow: 
    0 0 25px rgba(255, 255, 0, 0.8),
    inset 0 0 15px rgba(255, 255, 0, 0.3);
  transform: scale(1.1);
}

.missile-letter-image {
  transition: all 0.3s ease;
  border-radius: 5px;
}

.missile-letter.highlighted .missile-letter-image {
  filter: drop-shadow(0 0 20px #ffff00) brightness(1.3);
  transform: scale(1.1);
}

.missile-letter-text {
  font-family: "Press Start 2P", monospace;
}

.missile-letter.highlighted .missile-letter-text {
  text-shadow: 0 0 20px #ffff00;
  transform: scale(1.1);
}

/* Estela de fuego - ahora hacia arriba del misil */
.fire-trail {
  position: absolute;
  top: -90%; /* Arriba del misil en lugar de abajo */
  left: 50%;
  transform: translateX(-50%);
  width: 30px;
  height: 80px;
  z-index: 3;
  pointer-events: none;
}

.fire-particle {
  position: absolute;
  border-radius: 50%;
  animation-iteration-count: infinite;
  animation-timing-function: ease-out;
  filter: blur(4px); /* Aumentar blur general */
  box-shadow: 0 0 8px currentColor, 0 4px 12px rgba(255, 100, 0, 0.6); /* Blur adicional en dirección Y */
}

.fire-1 {
  width: 16px;
  height: 16px;
  background: radial-gradient(circle, #ff4500 0%, #ff6600 50%, transparent 100%);
  left: 7px;
  top: 64px; /* Más abajo en el trail (más cerca del misil) */
  animation: fireParticle1 0.4s infinite;
}

.fire-2 {
  width: 14px;
  height: 14px;
  background: radial-gradient(circle, #ff6600 0%, #ff8800 50%, transparent 100%);
  left: 8px;
  top: 52px;
  animation: fireParticle2 0.5s infinite;
}

.fire-3 {
  width: 12px;
  height: 12px;
  background: radial-gradient(circle, #ff8800 0%, #ffaa00 50%, transparent 100%);
  left: 9px;
  top: 40px;
  animation: fireParticle3 0.6s infinite;
}

.fire-4 {
  width: 10px;
  height: 10px;
  background: radial-gradient(circle, #ffaa00 0%, #ffcc00 50%, transparent 100%);
  left: 10px;
  top: 28px;
  animation: fireParticle4 0.8s infinite;
}

.fire-5 {
  width: 8px;
  height: 8px;
  background: radial-gradient(circle, #ffcc00 0%, #ffee00 50%, transparent 100%);
  left: 11px;
  top: 16px;
  animation: fireParticle5 1s infinite;
}

/* Animaciones invertidas - ahora van hacia arriba */
@keyframes fireParticle1 {
  0% {
    opacity: 1;
    transform: translateY(0) scale(1);
  }
  50% {
    opacity: 0.8;
    transform: translateY(-8px) scale(1.2);
  }
  100% {
    opacity: 0;
    transform: translateY(-16px) scale(0.8);
  }
}

@keyframes fireParticle2 {
  0% {
    opacity: 0.9;
    transform: translateY(0) scale(1);
  }
  50% {
    opacity: 0.7;
    transform: translateY(-10px) scale(1.1);
  }
  100% {
    opacity: 0;
    transform: translateY(-20px) scale(0.7);
  }
}

@keyframes fireParticle3 {
  0% {
    opacity: 0.8;
    transform: translateY(0) scale(1);
  }
  50% {
    opacity: 0.6;
    transform: translateY(-12px) scale(1);
  }
  100% {
    opacity: 0;
    transform: translateY(-24px) scale(0.6);
  }
}

@keyframes fireParticle4 {
  0% {
    opacity: 0.7;
    transform: translateY(0) scale(1);
  }
  50% {
    opacity: 0.5;
    transform: translateY(-14px) scale(0.9);
  }
  100% {
    opacity: 0;
    transform: translateY(-28px) scale(0.5);
  }
}

@keyframes fireParticle5 {
  0% {
    opacity: 0.6;
    transform: translateY(0) scale(1);
  }
  50% {
    opacity: 0.4;
    transform: translateY(-16px) scale(0.8);
  }
  100% {
    opacity: 0;
    transform: translateY(-32px) scale(0.4);
  }
}

/* Información del sector en la parte superior */
.sector-info {
  margin: 10px;
  z-index: 10;
}

.sector-panel {
  border: 3px solid #00ffff;
  border-radius: 20px;
  padding: 15px 30px;
  text-align: center;
  box-shadow: 
    0 0 30px rgba(0, 255, 255, 0.5),
    inset 0 0 20px rgba(0, 255, 255, 0.1);
  backdrop-filter: blur(4px);
  animation: sectorGlow 3s ease-in-out infinite alternate;
}

.sector-label {
  font-size: 8px;
  color: #ffaa00;
  text-shadow: 0 0 5px #ffaa00;
  margin-bottom: 8px;
  font-family: "Press Start 2P", monospace;
  letter-spacing: 2px;
}

.sector-name {
  font-size: 18px;
  margin-bottom: 8px;
  color: #ffaa00;
  text-shadow: 0 0 15px #ffaa00;
  font-family: "Press Start 2P", monospace;
  font-weight: normal;
}

.sector-description {
  font-size: 12px;
  color: #00ffff;
  opacity: 0.9;
  font-family: "Press Start 2P", monospace;
  font-weight: normal;
}

@keyframes sectorGlow {
  0% {
    box-shadow: 0 0 30px rgba(0, 255, 255, 0.5), inset 0 0 20px rgba(0, 255, 255, 0.1);
    border-color: #00ffff;
  }
  100% {
    box-shadow: 0 0 50px rgba(0, 255, 255, 0.7), inset 0 0 30px rgba(0, 255, 255, 0.2);
    border-color: #66ffff;
  }
}

/* Score display - números más pequeños */
.score-display {
  background: rgba(0, 0, 0, 0.7);
  border: 3px solid #00ffff;
  padding: 15px 25px;
  border-radius: 20px;
  text-align: center;
  min-width: 250px;
  box-shadow: 
    0 0 25px rgba(0, 255, 255, 0.4),
    inset 0 0 15px rgba(0, 255, 255, 0.1);
}

.score-display .instrument-label {
  font-size: 10px;
  color: #ffaa00;
  text-shadow: 0 0 8px #ffaa00;
  margin-bottom: 8px;
  font-family: "Press Start 2P", monospace;
  letter-spacing: 2px;
}

.score-value {
  font-size: 28px;
  color: #00ffff;
  text-shadow: 0 0 20px #00ffff;
  font-family: "Press Start 2P", monospace;
  font-weight: normal;
  letter-spacing: 3px;
  animation: scoreAnimation 0.5s ease-out;
}

/* Lives display con naves en miniatura */
.lives-display {
  background: rgba(0, 0, 0, 0.7);
  border: 2px solid #00ffff;
  padding: 15px 20px;
  border-radius: 15px;
  text-align: center;
  box-shadow: 
    0 0 15px rgba(0, 255, 255, 0.3),
